/*
    Noumenon: reverse string
    https://github.com/twied/noumenon
*/

var reverseString = function(s) {
    var result = "";
    for (var index, value : s) {
        result = result + s[length(s) - (index + 1)];
    }
    return result;
};

println(reverseString("Lorem ipsum dolor sit amet, consectetur adipisici elit, sed eiusmod tempor incidunt ut labore et dolore magna aliqua."));